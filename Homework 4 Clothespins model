   "cell_type": "code",
   "execution_count": 197,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def clever(σ,L,n):\n",
    "    H=np.zeros(n)\n",
    "    x=np.zeros(n)\n",
    "    for k in range(n):\n",
    "        x[k]=random.uniform(0,L-2*k*σ)\n",
    "    np.sort(x)\n",
    "    for k in range(n):\n",
    "        H[k]=x[k]+2*k*σ\n",
    "    return H"
   ]
  },
