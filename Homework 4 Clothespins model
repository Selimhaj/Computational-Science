    "first=[]\n",
    "pins=range(8,20)\n",
    "for n in pins:\n",
    "    H=naive(.1,10,int(10e5),n)\n",
    "    r=(len(H)/n)/N              #Ratio of successful trials (success over total)\n",
    "    first.append(1/r)           #Storing the average first successful sampled configuration\n",
    "plt.plot(pins,first)\n",
    "plt.xlabel('Number of pins')\n",
    "plt.ylabel('Trials before reaching a correct configuration')\n",
    "plt.show()"
   ]
